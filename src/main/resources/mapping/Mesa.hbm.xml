<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.tripodes.model.bean">

	<class name="Mesa" table="mesa">
		<id name="id" column="idMesa" type="integer">
			<generator class="increment" />
		</id>

		<property name="descripcion" column="descripcion" type="string" />
		<property name="cantVotantes" column="cantidadVotantes" type="integer" />
		<property name="idCircuito" column="idCircuito" type="integer" />
	</class>
</hibernate-mapping>