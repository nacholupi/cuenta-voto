<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.tripodes.model.bean">

	<class name="OpcionVoto" table="opcionvoto">
		<id name="id" column="idOpcionVoto" type="integer">
			<generator class="increment" />
		</id>

		<property name="orden" column="Orden" type="integer" />
		<property name="nroLista" column="NroLista" type="integer" />
		<property name="agrupacion" column="Agrupacion" type="string" />
		<property name="listaInt" column="ListaInt" type="string" />
		<property name="tieneDip" column="tieneDiputado" type="boolean" />
		<property name="tieneLeg" column="tieneLegislador" type="boolean" />
		<property name="tieneCons" column="tieneConsejal" type="boolean" />
	</class>
</hibernate-mapping>